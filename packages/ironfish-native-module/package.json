  "homepage": "https://github.com/iron-fish/mobile-wallet",
  "dependencies": {},
  "devDependencies": {
    "@types/react": "~18.2.45",
    "expo-module-scripts": "^3.4.1",
    "expo-modules-core": "^1.11.12"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  }
}
