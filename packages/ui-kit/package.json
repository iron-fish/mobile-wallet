    "copy-icons": "cpx 'src/components/Icon/**/*.svg' dist/components/Icon",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react-native-web": "^0.19.12"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "react-native": "^0.74"
  },
